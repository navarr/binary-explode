{
  "name": "navarr/binary-explode",
  "description": "Explode a number into it's binary components",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Navarr Barnier",
      "email": "me@navarr.me"
    },
    {
      "name": "Matthew Trimmer"
    },
    {
      "name": "James Corrao",
      "email": "james.pm.corrao@gmail.com",
      "homepage": "https://github.com/jpmc"
    }
  ],
  "require": {
    "php": "^7.1|^8"
  },
  "autoload": {
    "psr-4": {
      "Navarr\\BinaryExploder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Navarr\\BinaryExploder\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1",
    "infection/infection": "^0.24.0",
    "squizlabs/php_codesniffer": "^3.6",
    "jetbrains/phpstorm-attributes": "^1.0",
    "roave/security-advisories": "dev-master"
  },
  "scripts": {
    "test": [
      "phpunit --verbose --coverage-clover=coverage.clover"
    ]
  }
}
